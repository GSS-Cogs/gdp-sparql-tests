PREFIX qb:      <http://purl.org/linked-data/cube#>
PREFIX skos:    <http://www.w3.org/2004/02/skos/core#>
PREFIX ui:      <http://www.w3.org/ns/ui#>

SELECT ?scheme  
    WHERE {
    { 
        ?scheme a skos:ConceptScheme .
        ?concept a skos:Concept .
        ?scheme skos:member ?concept .
    }
}
# GROUP BY ?scheme